{
  "name": "remix-on-vercel",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "preview": "pnpm build && vercel dev",
    "typecheck": "tsc",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.+(js|jsx|css|ts|tsx|cjs|mjs)\"",
    "format": "pnpm prettier --list-different",
    "format:write": "pnpm prettier --write",
    "prepare": "husky",
    "validate": "run-p typecheck lint format"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "@remix-run/node": "2.11.2",
    "@remix-run/react": "2.11.2",
    "@remix-run/server-runtime": "2.11.2",
    "@vercel/remix": "2.11.2",
    "isbot": "^5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@remix-run/dev": "2.11.2",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-plugin-import-x": "^4.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma": "^5.19.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@9.9.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "*"
      ],
      "allowAny": [
        "*"
      ]
    }
  },
  "prisma": {
    "seed": "tsx prisma/seed"
  }
}
