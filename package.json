{
  "name": "remix-on-vercel",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "typecheck": "tsc",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.+(js|jsx|css|ts|tsx|cjs|mjs)\"",
    "format": "pnpm prettier --list-different",
    "format:write": "pnpm prettier --write",
    "prepare": "husky",
    "validate": "run-p typecheck lint format"
  },
  "dependencies": {
    "@remix-run/node": "2.10.0",
    "@remix-run/react": "2.10.0",
    "@remix-run/server-runtime": "2.10.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/remix": "2.10.0",
    "isbot": "^4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.10.0",
    "@remix-run/eslint-config": "2.10.0",
    "@types/node": "^22.5.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^9.9.1",
    "eslint-plugin-import-x": "^4.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.17.0",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@9.9.0"
}
